.managePwdFormOptions .radio-inline input[type="radio"] + span::before{content: "";display: inline-block;position: absolute;width: 15px;height: 15px;left: 0;top: 2px; margin-left: -18px;border: 1px solid #ccd8e8;border-radius: 0px;background-color: #fff;-webkit-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;-o-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;transition: border 0.15s ease-in-out, color 0.15s ease-in-out;}
.managePwdFormOptions .radio-inline input[type="radio"] + span::after{display: inline-block;line-height:14px;position: absolute;width:15px;height: 15px;left: 0;top: 2px; margin-left: -18px;text-align:center;font-size: 9px;color: #004b88;background:#fff;border:1px solid #ccd8e8;border-radius:0px;-webkit-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;-o-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;transition: border 0.15s ease-in-out, color 0.15s ease-in-out;}
.managePwdFormOptions label.radio-inline input[type="radio"]+ span::before{content:''; background:#fff url(../images/uncheck-mark.png) no-repeat center center; border-color:#becadc;}
.managePwdFormOptions label.radio-inline input[type="radio"]:checked + span::after{content:''; background:#fff url(../images/check-mark.png) no-repeat center center; border-color:#056bbe;}
.managePwdFormOptions label.radio-inline input[type="radio"]:checked + span span,.managePwdForm label.radio-inline input[type="radio"]:checked + span i {color:#056bbe !important}

.checkbox, .checkbox-inline{margin-top:10px !important; margin-left:0px;}
.checkbox input, .checkbox-inline input{margin-top:0}
.checkbox input[type="checkbox"] + span, .checkbox-inline input[type="checkbox"] + span{display: inline-block;vertical-align: middle;position: relative;padding-left:5px; font-family:'OpenSans';font-size:14px;}
.checkbox input[type="checkbox"] + span::before, .checkbox-inline input[type="checkbox"] + span::before{content: "";display: inline-block;position: absolute;width: 15px;height: 15px;left: 0;top: 2px; margin-left: -18px;border: 1px solid #ccd8e8;border-radius: 0px;background-color: #fff;-webkit-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;-o-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;transition: border 0.15s ease-in-out, color 0.15s ease-in-out;}
.checkbox input[type="checkbox"] + span::after, .checkbox-inline input[type="checkbox"] + span::after{display: inline-block;line-height:14px;position: absolute;width:15px;height: 15px;left: 0;top: 2px; margin-left: -18px;text-align:center;font-size: 9px;color: #004b88;background:#fff;border:1px solid #ccd8e8;border-radius:0px;-webkit-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;-o-transition: border 0.15s ease-in-out, color 0.15s ease-in-out;transition: border 0.15s ease-in-out, color 0.15s ease-in-out;}
.checkbox input[type="checkbox"], .checkbox-inline input[type="checkbox"]{opacity: 0;z-index: 1;}
.checkbox input[type="checkbox"]:checked + span::after, .checkbox-inline input[type="checkbox"]:checked + span::after{font-family: 'Glyphicons Halflings';content: "\e013";}
.checkbox input[type="checkbox"]:disabled + span, .checkbox-inline input[type="checkbox"]:disabled + span{opacity: 0.65;}
.checkbox input[type="checkbox"]:disabled + span::before, .checkbox-inline input[type="checkbox"]:disabled + span::before{background-color: #eeeeee;cursor: not-allowed;}
.selectCheck.checkbox input[type="checkbox"] + span::before, .selectCheck.checkbox-inline input[type="checkbox"] + span::after{top:8px;}

/* Default check and radio buttons */
label.radio-inline {position: relative;display: inline-block;padding-left: 0; margin-top:0px}
.radio-inline-label-radio{position: absolute;z-index: -1;width: 1px;height: 1px;opacity: 0;overflow: hidden}
.radio-inline-label-radio+.radio-inline-label-text{cursor: pointer;}
.radio-inline-label-radio+.radio-inline-label-text::before{font-size: .875em;display: inline-block;width: 20px;height: 20px;border: 1px solid #ccc;margin-right: 5px;content: "\00a0";background-color: white;text-align: center; vertical-align:text-top}
.radio-inline-label-radio+.radio-inline-label-text:hover::before{outline:none}
.radio-inline-label-radio:checked+.radio-inline-label-text::before{font-family: FontAwesome;content: "\f111"; color:#617a9d; line-height: 19px;}
.radio-inline-label-radio:focus+.radio-inline-label-text::before .radio-inline-label-radio:active+.radio-inline-label-text::before{border-color: #3bb8ff;}
.radio-inline-label-radio+.radio-inline-label-text::before {border-radius: 100%}


