.managePwdFormOptions .active label.radio-inline input[type="radio"] + span::before, .managePwdFormOptions label.radio-inline input[type="radio"]{display:none;}
.managePwdFormOptions label.radio-inline input[type="radio"] + span::before{width: 40px;height: 40px;right: -35px;top: -35px;border-radius: 50%;left:auto; border-color:#becadc;background-image:none;}
.managePwdFormOptions label.radio-inline input[type="radio"] + span::after{right: -35px;left: auto;width: 40px;height: 40px;top: -35px;border-radius: 50%; font-size:22px; padding-top:12px;}
