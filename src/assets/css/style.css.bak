/* Pointed Arrow Styles */
.dropdown-menu:after {content: '';position: absolute;border-style: solid;border-width: 0 10px 10px;border-color: #FFFFFF transparent;display: block;width: 0;z-index: 1;top: -9px;left: 17%;}
.dropdown-menu:before {content: '';position: absolute;border-style: solid;border-width: 0 10px 10px;border-color: #ccd8e8 transparent;display: block;width: 0;z-index: 0;top: -10px;left: 17%;}

/* Notofication Arrow Position */
.borderedIcon .dropdown-menu:after{left:3%; border-color:#f5f9fe transparent;}
.borderedIcon .dropdown-menu:before{left:3%}
.drafts .dropdown-menu:after{left:3%; border-color:#f5f9fe transparent;}
.drafts .dropdown-menu:before{left:3%}
.dropdown.pull-right .dropdown-menu:after{right:3%; left:auto; border-color:#f5f9fe transparent;}
