.temporary + .tooltip.top > .tooltip-arrow { border-top-color:#ff9800; }
.temporary + .tooltip.bottom > .tooltip-arrow { border-bottom-color:#ff9800; }

.noaction {background:#BFBDBD}
.noaction + .tooltip > .tooltip-inner {background-color: #BFBDBD}
.noaction + .tooltip.top > .tooltip-arrow { border-top-color:#BFBDBD; }
.noaction + .tooltip.bottom > .tooltip-arrow { border-bottom-color:#BFBDBD; }



.progress-bar-warning{background:#e79450;}
.progress{background:#dee7f4; margin:0px; min-width:110px;}
